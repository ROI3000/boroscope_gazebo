<robot name="boroscope" xmlns:xacro="http://wiki.ros.org/xacro">
    <xacro:macro name="boroscope" params="prefix">

  <xacro:property name="bolen" value="0.2" />
  <xacro:property name="borad" value="0.003" />


   <material name="blue">
     <color rgba="0 0 0.8 1"/>
