  </link>

  <joint name="base_fixed" type="fixed">
    <parent link="base_link"/>
    <child link="test_object"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>		
    <limit effort="0" velocity="0" lower="0" upper="0"/>
    <dynamics damping="0.0" friction="0.0"/>
  </joint>

  <link name="test_object">
